# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|
  
  config.vm.box = "ubuntu/trusty64"
  config.vm.hostname = "vm-io"
  config.vm.provision "shell", path: "provision.sh"
  
  config.vm.provider "virtualbox" do |vb|
	vb.name = "vm-io"
    vb.memory = 4096
	vb.cpus = 2
  end
  
end
